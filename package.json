{
  "name": "feuer",
  "version": "1.1.5",
  "description": "feuer mmorts thingy",
  "author": "jannik@feuer.io",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node -r dotenv/config server/main.js",
    "build:server": "tsc -p server",
    "build:client": "webpack --mode production",
    "build": "npm run build:server && npm run build:client",
    "lint": "eslint --cache && npx prettier . -c --config .prettierrc.json",
    "lint:fix": "eslint --cache --fix && npx prettier . -c -w --config .prettierrc.json",
    "dev:server": "tsc -w -p server",
    "dev:client": "webpack -w",
    "dev:database": "docker run -p 27017:27017 -v db:/data/db -e MONGO_INITDB_ROOT_USERNAME=admin -e MONGO_INITDB_ROOT_PASSWORD=admin mongo mongod"
  },
  "dependencies": {
    "@types/express": "^4.17.8",
    "@types/node": "^14.17.21",
    "@types/yargs": "^17.0.5",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.11.1",
    "mongodb": "^4.1.3",
    "typescript": "^4.5.2",
    "ts-loader": "^9.2.6",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "seedrandom": "^3.0.5",
    "socket.io": "^4.5.2",
    "winston": "^3.3.3",
    "yargs": "^17.2.1",
    "@semantic-release/git": "^10.0.0",
    "@types/react": "^17.0.36",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^4.3.8",
    "prettier": "2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "semantic-release": "^18.0.0",
    "socket.io-client": "^4.2.0",
    "firebase": "^9.5.0",
    "pixi-filters": "^3.1.1",
    "pixi-viewport": "4.11.1",
    "pixi.js": "5.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
