{
  "name": "feuer",
  "version": "1.1.0",
  "description": "feuer mmorts thingy",
  "author": "jannik@feuer.io",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node -r dotenv/config build/server/src/main.js",
    "build:server": "tsc",
    "build:client": "browserify build/client/js/main.js -o build/bundle.js --debug",
    "build": "npm run build:server && npm run build:client",
    "lint": "eslint --cache && npx prettier . -c --config .prettierrc.json",
    "lint:fix": "eslint --cache --fix && npx prettier . -c -w --config .prettierrc.json"
  },
  "dependencies": {
    "@types/express": "^4.17.8",
    "@types/express-session": "^1.17.0",
    "@types/fast-simplex-noise": "^3.0.0",
    "@types/node": "^14.17.21",
    "@types/pixi.js": "^5.0.0",
    "@types/socket.io": "^3.0.2",
    "@types/socket.io-client": "^3.0.0",
    "@types/yargs": "^17.0.5",
    "body-parser": "^1.19.0",
    "browserify": "^16.5.2",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "fast-simplex-noise": "^3.2.1",
    "firebase-admin": "^9.11.1",
    "handlebars": "^4.7.7",
    "mongodb": "^4.1.3",
    "pixi-filters": "^3.1.1",
    "pixi-viewport": "4.11.1",
    "pixi.js": "5.2.4",
    "seedrandom": "^3.0.5",
    "socket.io": "^4.2.0",
    "socket.io-client": "^4.2.0",
    "typescript": "^4.4.3",
    "winston": "^3.3.3",
    "yargs": "^17.2.1"
